{
  "name": "set-concurent-segment-search-setting",
  "operation": {
    "operation-type": "put-settings",
    "body": {
      "transient": {
          "search.concurrent_segment_search.enabled": "{{concurrent_segment_search_enabled | default('false')}}"
      }
    }
  }
},
{
  "operation": "hybrid-query-only-range",
  "warmup-iterations": 25,
  "iterations": 100
  {%- if not target_throughput %}
  ,"target-throughput": 6
  {%- elif target_throughput is string and target_throughput.lower() == 'none' %}
  {%- else %}
  ,"target-throughput": {{ target_throughput | tojson }}
  {%- endif %}
  {%- if search_clients is defined and search_clients %}
  ,"clients": {{ search_clients | tojson}}
  {%- endif %}
},
