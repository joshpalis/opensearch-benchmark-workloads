{
    "operation": {
      "operation-type": "delete-ml-model",
      "model-name": "{{ model_name | default('huggingface/sentence-transformers/all-mpnet-base-v2')}}"
    }
  },
  {
    "operation": {
      "operation-type": "register-ml-model",
      "model-name": "{{ model_name | default('huggingface/sentence-transformers/all-mpnet-base-v2')}}",
